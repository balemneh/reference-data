# Copy this file to .env and update with your values

# Database
DB_PASSWORD=refdata_pass

# Keycloak
KEYCLOAK_ADMIN_PASSWORD=admin

# Spring profiles
SPRING_PROFILES_ACTIVE=docker

# Java Options
JAVA_OPTS=-Xms512m -Xmx1024m

# Kafka
KAFKA_HEAP_OPTS=-Xms256m -Xmx512m